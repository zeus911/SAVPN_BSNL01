<?xml version="1.0" encoding="utf-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
  version="2.0">

  <navigation-rule>
    <from-view-id>/jsf/sov/serviceOrderView.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>search</from-outcome>
      <to-view-id>/jsf/sov/results.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/jsf/sov/serviceOrderView.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>reset</from-outcome>
      <to-view-id>/jsf/sov/serviceOrderView.xhtml</to-view-id>
      <redirect />
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/jsf/sov/results.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>reset</from-outcome>
      <to-view-id>/jsf/sov/serviceOrderView.xhtml</to-view-id>
      <redirect />
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/jsf/sov/results.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>error</from-outcome>
      <to-view-id>/jsf/sov/serviceOrderView.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/jsf/sov/results.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>search</from-outcome>
      <to-view-id>/jsf/sov/results.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <application>
    <resource-bundle>
      <base-name>jsf_resources.sovRB</base-name>
      <var>sov_msg</var>
    </resource-bundle>
  </application>

</faces-config>
